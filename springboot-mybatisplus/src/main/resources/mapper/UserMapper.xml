<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.mybatisplus.dao.UserMapper">
    <select id="selectUserById" resultType="com.example.mybatisplus.entity.User">
        select `id`,`name`,`age`,`email` from user where `id`=#{id}
    </select>
    <select id="selectMyWrapper" resultType="com.example.mybatisplus.entity.User">
        select *
        from user ${ew.customSqlSegment}
    </select>
</mapper>