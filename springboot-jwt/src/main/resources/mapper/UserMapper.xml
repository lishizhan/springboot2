<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.springbootjwt.dao.UserMapper">

    <select id="login" resultType="com.example.springbootjwt.pojo.User">
        select * from user where   `name`=#{name} and `pwd`=#{pwd} and `status`=0
    </select>
</mapper>