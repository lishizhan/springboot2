<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.dume.dataserver1.dao.MianTableDao" >

    <insert id="insertList">
        INSERT INTO main_table
        (id, heroname, age,operationtime,changedbname)
        VALUES
        <foreach collection ="list" item="item" separator =",">
            (#{item.id}, #{item.heroname}, #{item.age}, #{item.operationtime}, #{item.changedbname})
        </foreach >
    </insert>
  <select id="selectList" resultType="com.example.dume.dataserver1.model.MainTable">
      SELECT
	   *
      FROM
	  main_table
  </select>
</mapper>